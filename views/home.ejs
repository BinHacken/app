<!DOCTYPE html>
<html lang="de">

<head>
  <%- include('head'); %>
</head>

<body>
  <header>
    <%- include('header'); %>
  </header>

  <div class="container">
    <section>
      <h2>Willkommen bei <img style="height: 1rem;" src="images/logo.png"></h2>
      <ul>
        <li>Externe Links</li>
        <li>
          <% links.forEach(function(link){ %>
          <a class="button primary" href="<%= link.url %>" target="_blank"><%= link.name %></a>
          <% }); %>
        </li>
      </ul>
    </section>
  </div>
</body>

</html>